<%= form_with url: root_path, method: :post, local: true do |f| %>
  <div class="form-group">
    <%= f.label :address, "Enter Address" %>
    <%= f.text_field :address, class: "form-control", placeholder: "123 Main St, Springfield, IL" %>
  </div>

  <div class="form-group">
    <%= f.submit "Submit", class: "btn btn-primary" %>
  </div>
<% end %>

